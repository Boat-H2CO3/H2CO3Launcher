                        android:layout_height="0dp"
                        android:layout_weight="1"/>

                    <org.koishi.launcher.h2co3library.component.view.H2CO3LauncherButton
                        android:id="@+id/reset_cursor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/button_reset"
                        app:ripple="true"/>

                    <org.koishi.launcher.h2co3library.component.view.H2CO3LauncherButton
                        android:id="@+id/cursor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="10dp"
                        android:text="@string/button_set"
                        app:ripple="true"/>

                </androidx.appcompat.widget.LinearLayoutCompat>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@android:color/darker_gray"/>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="48dp"
                    android:orientation="horizontal"
                    android:paddingStart="12dp"
                    android:paddingTop="8dp"
                    android:paddingEnd="12dp"
                    android:paddingBottom="8dp">

                    <org.koishi.launcher.h2co3library.component.view.H2CO3LauncherTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:singleLine="true"
                        android:text="@string/settings_launcher_menu_icon"
                        app:auto_text_tint="true"/>

                    <View
